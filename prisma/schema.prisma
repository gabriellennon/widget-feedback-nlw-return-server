
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

// criando uma tabela no banco
model Feedback {
  // Colunas

  id String @id @default(uuid())

  type String
  comment String
  screenshot String?

  @@map("feedbacks")
}